{
    "name": "tuupola/base32",
    "description": "Base32 encoder and decoder for arbitrary data",
    "keywords": [
        "base32"
    ],
    "homepage": "https://github.com/tuupola/base32",
    "license": "MIT",
    "authors": [
        {
            "name": "Mika Tuupola",
            "email": "tuupola@appelsiini.net",
            "homepage": "https://appelsiini.net/",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.1|^8.0"
    },
    "suggest": {
        "ext-gmp": "GMP extension provides the fastest encoding and decoding."
    },
    "autoload": {
        "psr-4": {
            "Tuupola\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tuupola\\": "tests"
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "phpunit/phpunit": "^7.0|^8.0|^9.0",
        "overtrue/phplint": "^1.2",
        "phpbench/phpbench": "^0.16.0",
        "phpstan/phpstan": "^0.12.9"
    }
}
